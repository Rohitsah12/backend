{
  "manifest_version": 3,
  "name": "LeetCode Accepted Submission Tracker",
  "version": "1.0",
  "description": "Tracks accepted LeetCode submissions live for the user.",
  "permissions": [
    "storage",
    "scripting",
    "activeTab"
  ],
  "host_permissions": [
    "https://leetcode.com/*"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://leetcode.com/problems/*"],
      "js": ["content.js"]
    }
  ]
}
